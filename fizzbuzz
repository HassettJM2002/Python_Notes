#!/usr/bin/env python3
usernum = input('Enter a number ')
divisby5 = int(usernum) % 5
divisby3 = int(usernum) % 3
if (divisby5 == 0) and (divisby3 == 0):
    print('fizzbuzz')
elif (divisby3 == 0) and (divisby5 != 0):
    print('fizz')
elif (divisby3 != 0) and (divisby5 == 0):
    print('buzz')
else:
    print(usernum)
