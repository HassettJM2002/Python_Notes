#!/usr/bin/env python3
usernum = input('Enter a number ')
divisby5 = int(usernum) % 5
divisby3 = int(usernum) % 3

boolean = divisby5 + divisby3

if boolean == 0:
    print('fizzbuzz')
if boolean == 1:
    if divisby3 == 1:
    print('fizz')
    elif divisby5 == 1:
    print('buzz')
else:
    print(usernum)
    
